{
  "name": "mousetrap-keyboard-shortcuts",
  "version": "0.1.0",
  "description": "A react library for handling shortcut keys based on explicit priority in React applications. It has support for both class based components as well as functional components.",
  "keywords": [
    "keyboard",
    "shortcuts",
    "events",
    "react",
    "hotkeys"
  ],
  "main": "lib/hotkeys.js",
  "repository": "git@github.com:rajatgalav/mousetrap-keyboard-shortcuts.git",
  "author": "Rajat Galav <rajatgalav1295@gmail.com>",
  "scripts": {
    "build": "babel src --out-dir lib",
    "release": "npm run build && standard-version"
  },
  "dependencies": {
    "mousetrap": "^1.6.5"
  },
  "peerDependencies": {
    "lodash": ">= 4.17.4",
    "react": ">= 0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "standard-version": "^4.2.0"
  }
}
